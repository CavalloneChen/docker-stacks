{
    "@jupyterlab/shortcuts-extension:plugin":{
        "jupyterlab_code_formatter:black":{
        "command": "jupyterlab_code_formatter:black",
        "keys": [
            "Ctrl K",
            "Ctrl M"
        ],
        "selector": ".jp-Notebook.jp-mod-editMode"
    }
    },

    "@jupyterlab/notebook-extension:tracker":{
        "codeCellConfig": {
            "fontFamily": "Monaco",
            "fontSize": 11,
            "lineWrap": "wordWrapColumn",
            "wordWrapColumn": 120,
            "lineNumbers": true
          }
    },

    "@jupyterlab/github:drive":{
        "defaultRepo": "cavallonechen/Github-Reading"
    }
}